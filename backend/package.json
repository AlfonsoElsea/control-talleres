{
  "name": "@taller/leads-module",
  "version": "1.0.0",
  "description": "Módulo de gestión de leads para talleres de telefonía",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./core": {
      "import": "./dist/esm/core/index.js",
      "require": "./dist/cjs/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./api": {
      "import": "./dist/esm/api/index.js",
      "require": "./dist/cjs/api/index.js",
      "types": "./dist/api/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir dist/cjs",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir dist/esm",
    "build:types": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "npm run build && npm run test",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "taller",
    "telefonía",
    "leads",
    "reparación",
    "crm",
    "typescript"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "peerDependencies": {
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "zod": "^3.21.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu-usuario/taller-leads-module.git"
  },
  "bugs": {
    "url": "https://github.com/tu-usuario/taller-leads-module/issues"
  },
  "homepage": "https://github.com/tu-usuario/taller-leads-module#readme"
}
